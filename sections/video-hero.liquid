{% liquid
  assign has_video = false

  if section.settings.video != blank
    assign has_video = true
  endif

  assign show_animations = false
  assign section_class = 'video-hero section'

  if settings.enable_section_animations and section.settings.section_style_enable_animation
    assign section_class = section_class | append: ' animation animation--video-hero'
    assign show_animations = true
  endif
-%}

<div
  class="{{ section_class }}"
  data-section-id="{{ section.id }}"
  data-section-type="video-hero"
  data-image-height-type="{{ section.settings.image_height_type }}"
  data-has-video="{{ has_video }}"
  style="
    --video-focal-alignment: {{ section.settings.video_focal_point }};
    {% render 'settings-section-style-style' %}
    {% render 'settings-image-height-type-style' local_settings: section.settings %}
    {% render 'settings-text-position-style' local_settings: section.settings %}
    --content-max-width: {{ section.settings.content_max_width }}px;
  "
>
  <div
    class="
      section-inner
      {% if section.settings.enable_full_width %}section-inner--full-width{% endif %}
    "
  >
    <div class="video-hero__inner">
      <div class="video-hero__media-container">
        {% if section.settings.video != blank -%}
          {% liquid
            assign video_html = section.settings.video | video_tag: image_size: '1800x', class: 'video-hero__video animation--video', autoplay: true, loop: true, muted: true, controls: false
            assign video_poster_alt = '<img alt="' | append: video.title | append: '"'
            assign video_html = video_html | replace: '<img ', video_poster_alt
          -%}
          
          <div class="video-hero__video-wrapper">
            {{ video_html }}
            {% render 'video-pause' %}
          </div>
        {%- else -%}
          {% render 'image',
            wrapper_class: 'video-hero__image',
            sizes: '100vw',
            include_placeholder: true,
            placeholder_natural_fallback_ratio: 'no-ratio',
            placeholder_name: 'lifestyle-2',
            animate: show_animations
          -%}
        {%- endif %}
      </div>

      <div
        class="image-overlay video-hero__overlay animation--image-overlay"
        {%- render 'settings-image-overlay', local_settings: section.settings -%}
      ></div>

      <div class="video-hero__text-container-wrapper">
        {% unless section.settings.media_link == blank -%}
          <a
            href="{{ section.settings.media_link }}"
            class="video-hero__media-link"
            aria-label="{{ section.settings.media_link }}"
          ></a>
        {%- endunless %}

        {% liquid
          assign text_container_class = 'video-hero__text-container image-overlay-content section-blocks'

          unless section.settings.enable_text_box
            assign text_container_class = text_container_class | append: ' animation--container'
          endunless
        -%}

        {%- capture text_container_attributes -%}
          {% render 'text-box-container-class-and-attributes',
            local_settings: section.settings,
            class: text_container_class,
            animate_text_box: true,
            section_type: 'hero'
          -%}
        {%- endcapture -%}

        <div {{ text_container_attributes }}>
          {% liquid
            assign alignment = section.settings.text_position | split: '_' | first
            assign alignment_mobile = section.settings.text_position_mobile | split: '_' | first
          -%}

          {% render 'section-blocks',
            blocks: section.blocks,
            image_block_break_to_mobile_at: '480',
            alignment: alignment,
            alignment_mobile: alignment_mobile
          -%}
        </div>
      </div>
    </div>
  </div>
</div>


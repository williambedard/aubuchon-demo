{% liquid
  assign show_animations = false

  if settings.enable_section_animations and section.settings.section_style_enable_animation
    assign show_animations = true
  endif

  assign section_class = 'image-with-content media-with-content section'

  if show_animations
    assign section_class = section_class | append: ' animation animation--image-with-content'
  endif

  assign content_is_full_width = true

  if section.settings.content_width == 'custom'
    assign content_is_full_width = false
  endif

  # The below values don't include gutter, which is accounted for in the sizes string

  if section.settings.enable_image_2
    assign desktop_image_width = section.settings.image_width | divided_by: 100.0 | times: 0.6
    assign mobile_image_width = 0.6
  else
    assign desktop_image_width = section.settings.image_width | divided_by: 100.0
    assign mobile_image_width = 1
  endif

  assign image_sizes = '(min-width: 1024px) calc((100vw - (clamp(24px, 2vw, 36px) * 2)) * [[diw]]), (min-width: 720px) calc((100vw - (clamp(24px, 2vw, 36px) * 2)) * [[miw]]), calc((100vw - (16px * 2)) * [[miw]])' | replace: '[[diw]]', desktop_image_width | replace: '[[miw]]', mobile_image_width

  assign prioritize_loading = false
  if section.index == 1
    assign prioritize_loading = true
  endif
-%}

{% capture media -%}
  <div class="media-with-content__images-wrapper" data-has-two-images="{{ section.settings.enable_image_2 }}">
    {% render 'image',
      wrapper_class: 'media-with-content__image image-1',
      image: section.settings.image_1,
      sizes: image_sizes,
      aspect_ratio: section.settings.image_aspect_ratio_1,
      focal_point: section.settings.image_focal_point_1,
      include_placeholder: true,
      animate: show_animations,
      prioritize_loading: prioritize_loading
    -%}

    {% if section.settings.enable_image_2 -%}
      {% render 'image',
        wrapper_class: 'media-with-content__image image-2',
        image: section.settings.image_2,
        sizes: image_sizes,
        aspect_ratio: section.settings.image_aspect_ratio_2,
        focal_point: section.settings.image_focal_point_2,
        include_placeholder: true,
        animate: show_animations,
        prioritize_loading: prioritize_loading
      -%}
    {%- endif %}
  </div>

  {% if section.settings.image_caption_1 != blank -%}
    <div class="media-with-content__image-caption fs-body-75 animation--image-caption">
      {{ section.settings.image_caption_1 }}
    </div>
  {%- endif %}
{%- endcapture %}

{% capture content -%}
  {% render 'section-blocks',
    blocks: section.blocks,
    alignment: section.settings.content_alignment,
    full_width: content_is_full_width,
    image_block_break_to_mobile_at: '720'
  -%}
{%- endcapture %}

<div
  class="{{ section_class }}"
  data-section-id="{{ section.id }}"
  data-section-type="image-with-content"
  style="
    {% render 'settings-color-schemes-style', local_settings: section.settings %}
    {% render 'settings-section-style-style' %}
  "
>
  <div class="section-inner">
    {% render 'media-with-content',
      content: content,
      content_is_full_width: content_is_full_width,
      content_width: section.settings.custom_width,
      content_alignment: section.settings.content_alignment,
      media: media,
      media_container_width: section.settings.image_width,
      media_position_desktop: section.settings.image_position_desktop,
      media_position_mobile: section.settings.image_position_mobile,
      show_animations: show_animations
    -%}
  </div>
</div>


{%- liquid
  assign enable_locale_selector = section.settings.enable_locale_selector
  assign enable_country_selector = section.settings.enable_country_selector
  assign show_payment_icons = section.settings.show_payment_icons
  assign content = section.blocks

  assign locale_selector = false
  assign country_selector = false
  assign payment_icons = false

  if enable_locale_selector and shop.published_locales.size > 1
    assign locale_selector = true
  endif

  if enable_country_selector and localization.available_countries.size > 1
    assign country_selector = true
  endif

  if show_payment_icons and shop.enabled_payment_types != empty
    assign payment_icons = true
  endif
-%}

<div
  class="footer section"
  data-section-id="{{ section.id }}"
  data-section-type="footer"
  style="
    --color-background-input: {{ settings.color_text_footer | color_modify: 'alpha', 0.2 }};
    --color-background-input-highlight: {{ settings.color_text_footer | color_modify: 'alpha', 0.35 }};
    --color-text-input: {{ settings.color_text_footer | color_modify: 'alpha', 0.95 }};
    --desktop-column-gap: {{ section.settings.desktop_column_gap }}px;
    --color-text-alpha-40: {{ settings.color_text_footer | color_modify: 'alpha', 0.4 }};
  "
>
  <div class="section-inner">
    <div class="footer__inner">
      {% if content.size > 0 -%}
        <div class="footer__groups">
          {% for block in content -%}
            {% liquid
              assign prevBlock = null
              assign nextBlock = null
              assign nextIndex = forloop.index0 | plus: 1
              assign prevIndex = forloop.index0 | minus: 1

              unless forloop.first
                assign prevBlock = content[prevIndex]
              endunless

              unless forloop.last
                assign nextBlock = content[nextIndex]
              endunless

              assign rendered_image = blank

              if block.settings.image != blank or block.type == 'logo'
                capture rendered_image
                  render 'image', image: block.settings.image, wrapper_class: 'footer__image', sizes_px: block.settings.image_max_width, include_placeholder: true
                endcapture
              endif
            -%}

            {%- capture social_icons -%}
              {% if block.settings.show_social_icons -%}
                {% unless block.settings.social_icons_header == blank -%}
                  <h4 class="ff-heading fs-body-150 social-icons__heading">{{ block.settings.social_icons_header }}</h4>
                {%- endunless %}

                {% render 'social-icons' %}
              {%- endif %}
            {%- endcapture -%}

            {% liquid
              capture follow_on_shop_content
                if shop.features.follow_on_shop? and block.settings.enable_follow_on_shop
                  echo '<div class="footer__follow-on-shop">'
                  echo shop | login_button: action: 'follow'
                  echo '</div>'
                endif
              endcapture
            -%}

            <div
              class="footer__group"
              data-show-title-on-desktop="{{ block.settings.show_title_on_desktop }}"
              data-block-type="{{ block.type }}"
              data-prev-block-type="{{ prevBlock.type }}"
              data-next-block-type="{{ nextBlock.type }}"
              {% if block.settings.collapse_on_mobile and block.settings.title != blank -%}
                data-collapse-on-mobile="true"
              {%- endif %}
              style="
                {% if block.settings.image_max_width -%}
                  --image-max-width: {{ block.settings.image_max_width }}px;
                {%- endif %}
                {% if block.settings.custom_content_width -%}
                  --custom-content-width: {{ block.settings.custom_content_width }}px;
                {%- endif %}
              "
            >
              {% if block.settings.title != blank -%}
                <h3 class="footer__header ff-body{% if block.settings.collapse_on_mobile %} footer__header--collapsed{% endif %}">
                  <span class="footer__heading ff-heading fs-heading-5-base">{{ block.settings.title }}</span>
                </h3>

                {% if block.settings.collapse_on_mobile -%}
                  <button type="button" class="footer__header ff-body" data-header>
                    <span class="footer__heading ff-heading fs-heading-6-base">{{ block.settings.title }}</span>
                    <span class="footer__arrow">{% render 'icon', icon: 'chevron-small' %}</span>
                  </button>
                {%- endif %}
              {%- endif %}

              <div class="footer__group-content">
                {%- case block.type -%}
                  {%- when 'logo' -%}
                    {% liquid
                      echo rendered_image
                      echo social_icons
                      echo follow_on_shop_content
                    -%}
                  {%- when 'text' -%}
                    {% liquid
                      if block.settings.image_placement == 'above_text'
                        echo rendered_image
                      endif

                      echo '<div class="ff-body fs-body-100 footer__text">[[bst]]</div>' | replace: '[[bst]]', block.settings.text

                      if block.settings.image_placement == 'below_text'
                        echo rendered_image
                      endif

                      echo social_icons
                      echo follow_on_shop_content
                    -%}
                  {%- when 'links' -%}
                    <div class="footer__links fs-body-100">
                      {% if linklists[block.settings.link_list].links.size > 0 %}
                        <ul>
                          {% for link in linklists[block.settings.link_list].links %}
                            <li>
                              <a href="{{ link.url }}">{{ link.title }}</a>
                            </li>
                          {% endfor %}
                        </ul>
                      {% endif %}
                    </div>
                  {%- when 'liquid' -%}
                    <div class="footer__liquid fs-body-100">
                      {{ block.settings.liquid }}
                    </div>
                  {%- when 'contact' -%}
                    <div class="ff-body fs-body-100 footer__text">{{ block.settings.text }}</div>

                    {% if block.settings.contact_text_1 != blank
                      or block.settings.contact_text_2 != blank
                      or block.settings.contact_text_3 != blank
                    -%}
                      <div class="footer-contact-info">
                        {% for i in (1..3) -%}
                          {% liquid
                            assign contact_text = 'contact_text_' | append: i
                            assign contact_icon = 'contact_icon_' | append: i
                            assign contact_link = 'contact_link_' | append: i
                          -%}

                          {% if block.settings[contact_text] != blank -%}
                            <p>
                              {% liquid
                                if block.settings[contact_icon] != 'none'
                                  render 'icon-editorial', icon: block.settings[contact_icon]
                                endif

                                capture tag_name
                                  if block.settings[contact_link] != blank
                                    echo 'a'
                                  else
                                    echo 'span'
                                  endif
                                endcapture

                                capture conditional_attributes
                                  if block.settings[contact_link] != blank
                                    echo 'href="[[url]]"' | replace: '[[url]]', block.settings[contact_link]
                                  endif
                                endcapture
                              -%}

                              <{{ tag_name }} {{ conditional_attributes }}>
                                {{ block.settings[contact_text] | newline_to_br }}
                              </{{ tag_name }}>
                            </p>
                          {%- endif %}
                        {%- endfor %}
                      </div>
                    {%- endif %}

                    {{ social_icons }}
                    {{ follow_on_shop_content }}
                  {%- when 'newsletter' -%}
                    {%- assign success_msg = 'general.newsletter_form.confirmation' | t -%}

                    <div class="footer__newsletter">
                      <div class="ff-body fs-body-100 footer__text">{{ block.settings.text }}</div>

                      {% form 'customer', class: 'footer__newsletter-form', id: 'footer-subscribe' -%}
                        {% render 'form-status',
                          form: form,
                          form_id: 'footer-subscribe',
                          success_message: success_msg
                        %}

                        {% unless form.posted_successfully? -%}
                          <div class="footer__newsletter-inner">
                            <input type="hidden" name="contact[tags]" value="newsletter">

                            <label for="footer-subscribe-email" class="visually-hidden">
                              {{- 'general.newsletter_form.email_placeholder' | t -}}
                            </label>

                            <input
                              type="email"
                              class="input"
                              name="contact[email]"
                              id="footer-subscribe-email"
                              value="{% if customer.email != blank %}{{ customer.email }}{% endif %}"
                              placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}"
                              autocorrect="off"
                              autocapitalize="off"
                              required
                            >

                            <button
                              type="submit"
                              name="commit"
                              id="Subscribe-footer"
                              class="footer__newsletter-button btn btn--primary"
                              aria-label="{{ 'general.newsletter_form.submit' | t }}"
                              style="
                                --color-btn-text: var(--color-background-footer);
                                --color-btn-bg: var(--color-text-footer);
                                --color-btn-bg-hover: var(--color-text-footer-subdued);
                              "
                            >
                              {{ 'general.newsletter_form.submit' | t }}
                            </button>
                          </div>
                        {%- endunless %}
                      {%- endform %}

                      {{ social_icons }}
                      {{ follow_on_shop_content }}
                    </div>
                {%- endcase -%}
              </div>
            </div>
          {%- endfor %}
        </div>
      {%- endif %}

      <div class="footer__bottom">
        <div class="footer__bottom-left">
          {% liquid
            if locale_selector or country_selector
              render 'cross-border', show_locale: locale_selector, show_country: country_selector, id: 'footer'
            endif
          -%}

          <div class="footer__credits fs-body-75">
            {%- liquid
              capture powered_by_content
                echo powered_by_link | replace: 'rel="nofollow"', 'rel="nofollow noopener"'
              endcapture

              capture powered_by_content_stripped
                echo powered_by_content | strip_html | strip
              endcapture
            -%}

            <p>
              {{ 'sections.footer.copyright' | t }} &copy; {{ 'now' | date: '%Y' }}, {{ shop.name | link_to: '/' }}

              {%- unless powered_by_content_stripped == blank -%}
                &mdash;
                {{ powered_by_content }}
              {%- endunless -%}
            </p>

            <span class="footer__credits-additional-text">
              {{ section.settings.additional_copyright_text }}
            </span>
          </div>
        </div>

        {% if linklists[section.settings.subfooter_menu].links.size > 0 -%}
          <div class="footer__bottom-center">
            <ul class="footer__subfooter-menu fs-body-75">
              {% for link in linklists[section.settings.subfooter_menu].links -%}
                <li>
                  <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
              {%- endfor %}
            </ul>
          </div>
        {%- endif %}

        {% if payment_icons -%}
          <div class="footer__bottom-right">
            {% render 'payment-icons' %}
          </div>
        {%- endif %}
      </div>
    </div>
  </div>
</div>


{% liquid
  assign show_animations = false
  assign section_class = 'collapsible-row-list section'
  assign flex_direction = 'column'
  assign header_order = 1
  assign content_order = 2

  if settings.enable_section_animations and section.settings.section_style_enable_animation
    assign show_animations = true
  endif

  if show_animations
    assign section_class = section_class | append: ' animation animation--collapsible-row-list'
  endif

  if section.settings.header_position == 'left'
    assign flex_direction = 'row'
  elsif section.settings.header_position == 'right'
    assign flex_direction = 'row'
    assign header_order = 2
    assign content_order = 1
  endif

  assign content_styles = '--content-flex-direction: [[cfd]];' | replace: '[[cfd]]', flex_direction
  assign content_styles = content_styles | append: ' --header-order: [[ho]];' | replace: '[[ho]]', header_order
  assign content_styles = content_styles | append: ' --content-order: [[co]];' | replace: '[[co]]', content_order

  if section.settings.content_width == 'custom'
    assign content_styles = content_styles | append: ' --content-width: [[cw]]px;' | replace: '[[cw]]', section.settings.custom_width
  endif
-%}

<div
  class="{{ section_class }}"
  data-section-id="{{ section.id }}"
  data-section-type="collapsible-row-list"
  data-use-custom-colors="{{ section.settings.override_with_custom_colors }}"
  data-color-scheme="{{ section.settings.color_scheme }}"
  style="
    {% render 'settings-color-schemes-style', local_settings: section.settings %}
    {% render 'settings-section-style-style' %}
  "
>
  <div class="section-inner">
    <div
      class="collapsible-row-list__inner"
      style="
        {{ content_styles }}
        {% render 'settings-color-schemes-style', local_settings: section.settings %}
      "
      data-flex-direction="{{ flex_direction }}"
    >
      {% render 'settings-section-header-content', section: section %}

      <div class="collapsible-row-list__items animation--container">
        {% for block in section.blocks -%}
          {% render 'collapsible-row-list-item',
            block: block,
            heading_font: section.settings.item_heading_font,
            forloop: forloop
          -%}
        {%- endfor %}
      </div>
    </div>
  </div>
</div>

